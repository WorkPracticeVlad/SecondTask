@section scripts {
    <script src="~/Scripts/MyTreeJS.js"></script>
}
@{
    ViewBag.Title = "TreePages";
}
<h2>TreePages</h2>
<div class="row" id="pages">
    <div class="col-md-12" data-bind="visible: viewMode() == 'propertiesMode', template : {name : 'propertiesTemplate'}"></div>
    @*<div class="col-md-12" data-bind="visible: viewMode() =='unitsMode', template : {name : 'unitsTemplate'}"></div>
    <div class="col-md-12" data-bind="visible: viewMode() =='valuesMode', template : {name : 'valuesTemplate'}"></div>*@
</div>
<script type="text/html" id="propertiesTemplate">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th class="col-md-6">Name</th>
                <th class="col-md-6">Type</th>
            </tr>
        </thead>
        <tbody data-bind="foreach : props">
            <tr>
                <td class="col-md-6" data-bind="text: $data.Name"></td>
                <td class="col-md-6" data-bind="text: $data.Type"></td>
            </tr>
        </tbody>
    </table>
    <div data-bind="text: numberOfPagesProps"></div>
    <div data-bind="foreach : pagesProps"><button data-bind="text : $data , click : $parent.LoadPageProps"></button>,</div>
</script>